# Go Redirect Configuration

# Target URL (required)
REDIRECT_TARGET=https://example.com

# HTTP status code (301 = permanent, 302 = temporary)
REDIRECT_CODE=301

# Preserve path and query string
PRESERVE_PATH=true

# Block common scanner paths (returns 404)
BLOCK_SCANNERS=true

# Custom blocked paths file (optional)
# BLOCKED_PATHS_FILE=/scanner_paths.json

# Log level (debug, info, warn, error, none)
LOG_LEVEL=info

# Server port
PORT=8080
